{
  "name": "custom-nx-electron",
  "version": "15.0.0",
  "main": "src/index.js",
  "description": "Custom Electron Plugin for Nx v15.x",
  "author": "HaHT",
  "license": "Apache-2.0",
  "generators": "./generators.json",
  "schematics": "./generators.json",
  "executors": "./executors.json",
  "builders": "./executors.json",
  "ng-update": {
    "requirements": {},
    "migrations": "./migrations.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hafht/custom-nx-electron.git"
  },
  "scripts": {
    "lint": "nx lint nx-electron",
    "lint:fix": "npm run lint --fix",
    "test": "nx test nx-electron",
    "build": "cd ../.. && nx build nx-electron",
    "release": "standard-version",
    "release:alpha": "standard-version",
    "release:beta": "standard-version",
    "postrelease": "git push --follow-tags && npm run build && cd ../../dist/packages/nx-electron && npm publish",
    "postrelease:alpha": "git push --follow-tags && npm run build && cd ../../dist/packages/nx-electron && npm publish --tag alpha",
    "postrelease:beta": "git push --follow-tags && npm run build && cd ../../dist/packages/nx-electron && npm publish --tag beta"
  },
  "keywords": [
    "nx",
    "electron",
    "forge",
    "package",
    "make",
    "bundle",
    "monorepo",
    "schematics",
    "generators",
    "builders",
    "executors"
  ],
  "peerDependencies": {
    "@nrwl/workspace": "^14.8.6",
    "electron": "*",
    "nx": "14.8.6",
    "tslib": "2.4.1"
  },
  "dependencies": {
    "circular-dependency-plugin": "^5.2.2",
    "copy-webpack-plugin": "10.2.4",
    "electron-builder": "^23.6.0",
    "fork-ts-checker-webpack-plugin": "7.2.13",
    "license-webpack-plugin": "4.0.2",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "rimraf": "3.0.2",
    "rxjs-for-await": "^1.0.0",
    "source-map-support": "0.5.19",
    "strip-json-comments": "3.1.1",
    "terser-webpack-plugin": "5.3.6",
    "tree-kill": "1.2.2",
    "ts-loader": "9.4.2",
    "tsconfig-paths-webpack-plugin": "3.5.2",
    "webpack": "5.75.0",
    "webpack-dev-server": "4.11.1",
    "webpack-merge": "5.7.3",
    "webpack-node-externals": "3.0.0"
  },
  "devDependencies": {
    "webpack-node-externals": "^3.0.0"
  },
  "types": "./src/index.d.ts"
}
